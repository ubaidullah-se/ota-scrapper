{
  "manifest_version": 3,
  "name": "Proxy Rotater",
  "version": "1.0",
  "description": "An extension to set a proxy server.",
  "permissions": [
    "tabs",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "webRequestAuthProvider",
    "storage",
    "<all_urls>",
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"]
}
